{
    "builds": [
      {
        "src": "./server/dist/server.js",
        "use": "@vercel/node"
      },
      {
        "src": "./client/dist",
        "use": "@vercel/static"
      }
    ],
    "rewrites": [
      {
        "source": "/(.*)",
        "destination": "/server/dist/server.js"
      }
    ],
    "trailingSlash": true
  }